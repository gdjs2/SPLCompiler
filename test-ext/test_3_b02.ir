FUNCTION findMax :
PARAM ec
max := #0
i := #0
n := #8
LABEL label1 :
t1 := i
t2 := n
IF t1 < t2 GOTO label2
GOTO label3
LABEL label2 :
t3 := max
t5 := i * #4
t6 := &c + t5
t4 := *t6
IF t3 < t4 GOTO label4
GOTO label5
LABEL label4 :
t8 := i * #4
t9 := &c + t8
max := *t9
LABEL label5 :
t11 := i
t12 := #1
i := t11 + t12
GOTO label1
LABEL label3 :
t13 := max
RETURN t13
